<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="DataPath1" value="..\..\..\Data\measurements_session.csv"/>
		<add key="DataPath2" value="..\..\..\Data\rejects.csv"/>

		<add key="PM_threshold" value="3.0"/> <!--0.05-->
		<add key="Stator_w_threshold" value="0.05"/> <!--0.05-->
		<add key="Stator_t_threshold" value="0.05"/> <!--0.05-->
		

		<add key="OutOfBandPercent" value="0.25"/> <!--0.01-->
	</appSettings>

	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding name="MyTcpBinding"
						 transferMode="Streamed"
						 maxReceivedMessageSize="104857600"
						openTimeout="00:01:00"
						closeTimeout="00:01:00"
						receiveTimeout="00:10:00"
						sendTimeout="00:10:00">
					<readerQuotas maxDepth="32"
								  maxStringContentLength="8192"
								  maxArrayLength="104857600"
								  maxBytesPerRead="4096"
								  maxNameTableCharCount="16384" />
				</binding>
			</netTcpBinding>
		</bindings>
		<services>
			<service name="Server.PMSMService">
				<endpoint address="ServicePMSM"
						  binding="netTcpBinding"
						  bindingConfiguration="MyTcpBinding"
						  contract="Common.IPMSMService">
				</endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002"/>
					</baseAddresses>
				</host>
			</service>
		</services>
	</system.serviceModel>
</configuration>